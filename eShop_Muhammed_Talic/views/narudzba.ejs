<!DOCTYPE html>
<html lang="bs">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">

      <title> <%= title %> </title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script>

    <!-- Font Awesome -->
    <link
            href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css"
            rel="stylesheet"
    />
    <!-- Google Fonts -->
    <link
            href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap"
            rel="stylesheet"
    />
    <!-- Bootstrap -->
    <link
            href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.10.1/mdb.min.css"
            rel="stylesheet"
    />
    <style>
        body {
            background-color: #fbfbfb;
        }
        @media (min-width: 991.98px) {
            main {
                padding-left: 240px;
            }
        }

        /* Sidebar */
        .sidebar {
            position: fixed;
            top: 60px  ;
            /*background-color: #405fb2;*/
            bottom: 0;
            left: 0;
            /*padding: 58px 0 0; !* Height of navbar *!*/
            /*box-shadow: 0 2px 5px 0 #bcc0cb, 0 2px 10px 0 #164979;*/
            width: 240px;
            z-index: 600;
        }

        @media (max-width: 991.98px) {
            .sidebar {
                width: 100%;
            }
        }
        .sidebar .active {
            border-radius: 5px;
            box-shadow: 0 2px 5px 0 #0942dc, 0 2px 10px 0 #164979;
        }
    </style>
</head>
<body>

<div >
    <%- include('include/_navBarnew.ejs') %>
</div>
<div class="container-fluid ">
    <header>
        <!-- Sidebar -->
        <nav
                id="sidebarMenu"
                class="collapse d-lg-block sidebar collapse bg-white">
            <div class="position-sticky">
                <div class="list-group list-group-flush mx-3 mt-4">
                    <!-- Collapse 1 -->
                    <a
                            class="list-group-item list-group-item-action py-2 ripple"
                            aria-current="true"
                            data-mdb-toggle="collapse"
                            href="#collapseExample1"
                            aria-expanded="true"
                            aria-controls="collapseExample1"
                    >
                        <i class="fas fa-tachometer-alt fa-fw me-3"></i
                        ><span>Trgovina</span>
                    </a>
                    <!-- Collapsed content -->
                    <ul
                            id="collapseExample1"
                            class="collapse show list-group list-group-flush"
                    >
                        <li class="list-group-item py-1">
                            <a href="/TalaShop/narudzba"  class="text-reset"> Nove narudžbe </a>
                        </li>
                        <li class="list-group-item py-1">
                            <a href="/TalaShop/narudzba?status=Odobreno"  class="text-reset"> Odobrene narudžbe </a>
                        </li>
                        <li class="list-group-item py-1">
                            <a href="/TalaShop/narudzba?status=Odbijeno"  class="text-reset"> Odbijene narudžbe </a>
                        </li>
                    </ul>
                    <!-- Collapse 1 -->

                    <!-- Collapse 2 -->
                    <a
                            class="list-group-item list-group-item-action py-2 ripple"
                            aria-current="true"
                            data-mdb-toggle="collapse"
                            href="#collapseExample2"
                            aria-expanded="true"
                            aria-controls="collapseExample2"
                    >
                        <i class="fas fa-chart-area fa-fw me-3"></i
                        ><span>Kupovina</span>
                    </a>
                    <!-- Collapsed content -->
                    <ul
                            href="/TalaShop/narudzba/korpa"
                            id="collapseExample2"
                            class="collapse list-group list-group-flush"
                    >
                        <li class="list-group-item py-1">
                            <a href="/TalaShop/narudzba?status=Poslato_kupovina"  class="text-reset"> Nove narudžbe </a>
                        </li>
                        <li class="list-group-item py-1">
                            <a href="/TalaShop/narudzba?status=Odobreno_kupovina"  class="text-reset"> Odobrene narudžbe </a>
                        </li>
                        <li class="list-group-item py-1">
                            <a href="/TalaShop/narudzba?status=Odbijeno_kupovina"  class="text-reset"> Odbijene narudžbe </a>
                        </li>
                    </ul>
                    <!-- Collapse 2 -->
                    <!-- Collapse 3 -->
                    <a
                            class="list-group-item list-group-item-action py-2 ripple"
                            aria-current="true"
                            data-mdb-toggle="collapse"
                            href="#collapseExample3"
                            aria-expanded="true"
                            aria-controls="collapseExample3"
                    >
                        <i class="fas fa-chart-area fa-fw me-3"></i
                        ><span>Statistika</span>
                    </a>
                    <!-- Collapsed content -->
                    <ul
                            href="/TalaShop/narudzba/korpa"
                            id="collapseExample3"
                            class="collapse list-group list-group-flush"
                    >
                        <li class="list-group-item py-1">
                            <a href="#"  class="text-reset"> Prodaja  </a>
                        </li>
                        <li class="list-group-item py-1">
                            <a href="#"  class="text-reset"> Kupovina  </a>
                        </li>
                    </ul>
                    <!-- Collapse 2 -->
                </div>
            </div>
        </nav>
        <!-- Sidebar -->
    </header>
    <!--Main layout-->
    <main>
        <div class="container p-0">
            <% switch (title) {
            case 'Odobreno' : %>
            <%- include('include/_narudzbe_odobreno.ejs') %>
                    <% break;

            case 'Odbijeno' : %>
            <%- include('include/_narudzbe_odbijeno.ejs') %>
                    <% break;

            case 'Odbijeno_kupovina' : %>
            <%- include('include/_narudzbe_odbijeno_kupovina.ejs') %>
                    <% break;

            case 'Odobreno_kupovina' : %>
            <%- include('include/_narudzbe_odobreno_kupovina.ejs') %>
                    <% break;

            case 'Poslato_kupovina' : %>
            <%- include('include/_narudzbe_poslato_kupovina.ejs') %>
                    <% break;

            default : %>
            <%- include('include/_narudzbe_poslato.ejs') %>
                    <% ;

            } %>

        </div>
    </main>
    <!--Main layout-->
</div>
<script>

</script>
</body>
<script
        type="text/javascript"
        src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.10.1/mdb.min.js"
></script>
</html>
